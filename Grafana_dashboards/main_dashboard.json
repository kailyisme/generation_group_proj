        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "with best_selling as (\n  select \n    store.location as store_name,\n    case \n      when product_type.name = 'Unidentified'\n        THEN product.name\n      else product_type.name + ' ' + product.name\n    END as nname,\n    sum(product.price) as price_sum,\n    ROW_NUMBER() OVER (PARTITION by store.location order by sum(product.price) DESC) as rn\n    from transaction\n    join basket on basket.transaction_id = transaction.id\n    join product on product.id = basket.product_id\n    join product_type on product_type.id = product.type_id\n    JOIN store on store.id = transaction.store_id\n    where $__unixEpochFilter(transaction.datetime)\n    group by store.location, nname\n  )\n\n\nSELECT\n  best_selling.store_name,\n  best_selling.nname,\n  best_selling.price_sum\n  from best_selling\n  where rn < 4\n  order by best_selling.store_name, best_selling.price_sum DESC",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Top Grossing Products per Store",
      "type": "bargauge"
